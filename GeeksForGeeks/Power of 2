class Solution{
    public:
    // Function to check if given number n is a power of two.
    bool isPowerofTwo(long long n){
        
        for(int i=0;i<=64;i++)
        {
            // int ans=pow(2,i);
            if(pow(2,i)==n)
            return true;
        }
        return false;;
        
    }
};
